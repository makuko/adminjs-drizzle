{
  "name": "adminjs-drizzle",
  "version": "0.0.0",
  "description": "Drizzle ORM adapter for AdminJS",
  "keywords": [
    "adminjs",
    "drizzle",
    "admin"
  ],
  "license": "MIT",
  "author": "Markus Schmidt <markus.schmidt.84@gmail.com>",
  "repository": "makuko/adminjs-drizzle",
  "type": "module",
  "exports": {
    "mysql": {
      "types": "./dist/mysql/index.d.ts",
      "default": "./dist/mysql/index.js"
    },
    "pg": {
      "types": "./dist/pg/index.d.ts",
      "default": "./dist/pg/index.js"
    },
    "sqlite": {
      "types": "./dist/sqlite/index.d.ts",
      "default": "./dist/sqlite/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "start:mysql": "tsx test/mysql/index.ts",
    "start:pg": "tsx test/pg/index.ts",
    "start:sqlite": "tsx test/sqlite/index.ts",
    "generate:mysql": "drizzle-kit generate --config=drizzle.config.mysql.ts",
    "generate:pg": "drizzle-kit generate --config=drizzle.config.pg.ts",
    "generate:sqlite": "drizzle-kit generate --config=drizzle.config.sqlite.ts"
  },
  "peerDependencies": {
    "adminjs": ">=7",
    "drizzle-orm": ">=0.44"
  },
  "devDependencies": {
    "@adminjs/fastify": "^4.2.0",
    "@electric-sql/pglite": "^0.3.4",
    "@eslint/js": "9.30.1",
    "@libsql/client": "^0.15.9",
    "@tiptap/core": "^2.24.2",
    "adminjs": "^7.8.16",
    "dotenv": "^17.0.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "eslint": "^9.30.1",
    "fastify": "^5.4.0",
    "mysql2": "^3.14.1",
    "semantic-release": "^24.2.6",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}
